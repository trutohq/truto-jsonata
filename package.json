{
  "name": "@truto/truto-jsonata",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "version": "1.0.0",
  "description": "Truto extension of jsonata",
  "repository": "https://github.com/trutohq/truto-jsonata.git",
  "source": "src/index.ts",
  "main": "dist/main.cjs",
  "module": "dist/module.js",
  "types": "dist/types.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "require": "./dist/main.cjs",
      "import": "./dist/module.js"
    }
  },
  "targets": {
    "main": {
      "includeNodeModules": [
        "lodash-es"
      ],
      "isLibrary": true
    },
    "module": {
      "includeNodeModules": [
        "lodash-es"
      ],
      "isLibrary": true
    }
  },
  "type": "module",
  "license": "MIT",
  "author": "archith.s",
  "volta": {
    "node": "18.13.0",
    "yarn": "1.22.19"
  },
  "devDependencies": {
    "@types/jest": "^29.5.13",
    "@types/lodash-es": "4.17.7",
    "@types/luxon": "^3.4.2",
    "@types/node": "18.14.6",
    "eslint": "^9.13.0",
    "parcel": "2.8.3",
    "prettier": "^3.3.3",
    "typescript": "4.9.5",
    "vitest": "0.33.0"
  },
  "dependencies": {
    "@mixmark-io/domino": "2.2.0",
    "dependency-graph": "0.11.0",
    "dice-coefficient": "2.1.1",
    "hono": "3.11.8",
    "json2md": "2.0.0",
    "jsonata": "2.0.4",
    "lodash-es": "4.17.21",
    "luxon": "3.3.0",
    "marked": "13.0.1",
    "mime": "4.0.1",
    "turndown": "7.2.0",
    "turndown-plugin-gfm": "1.0.2",
    "vitest-fetch-mock": "^0.2.2",
    "xml-js": "1.6.11"
  },
  "scripts": {
    "dev": "parcel watch",
    "build": "rm -rf dist && parcel build",
    "check": "tsc --noEmit",
    "prepublishOnly": "yarn build",
    "test": "vitest"
  },
  "resolutions": {
    "word-wrap": "npm:@aashutoshrathi/word-wrap@1.2.5",
    "semver": "npm:semver@7.5.4"
  }
}
